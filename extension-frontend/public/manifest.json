{
  "manifest_version": 3,
  "name": "Airbnb flight",
  "version": "0.1.1", 
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [],
  "background": {
    "service_worker": "./js/background.js"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Airbnb flight",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.airbnb.co*/*"],
      "js": ["./js/content.js"],
      "run_at": "document_end"
    }
  ]
}