{
  "manifest_version": 3,
  "name": "Airbnb flight",
  "version": "0.1.1", 
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [],
  "background": {
    "service_worker": "./extension-scripts/background/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.airbnb.co.uk/"],
      "js": ["./extension-scripts/content/content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["./extension-scripts/content/*.js"],
      "matches": ["<all_urls>"]
    }
  ]
}