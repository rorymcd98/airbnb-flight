{
  "manifest_version": 3,
  "name": "Airbnb flight",
  "version": "0.1.1",
  "permissions": [
  ],
  "action": {
    "default_popup": "./extension-frontend/public/popup.html"
  },
  "background": {
    "service_worker": "./extension-scripts/background/background.ts"
  },
  "content_scripts": [
    {
      "matches": ["https://www.airbnb.co.uk/*"],
      "js": ["./extension-scripts/content/content.tsx"],
      "run_at": "document_start"
    }
  ]
}